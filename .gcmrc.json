{
  "rules": {
    "type-empty": [2, "never"],
    "type-enum": [
      2,
      "always",
      ["feat", "fix", "docs", "style", "refactor", "test", "chore", "revert"]
    ],
    "subject-empty": [2, "never"],
    "subject-max-length": [2, "always", 72],
    "subject-min-length": [2, "always", 5],
    "body-leading-blank": [2, "always"],
    "body-max-length": [2, "always", 72]
  }
}
